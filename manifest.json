{
  "manifest_version": 2,
  "name": "DuoHear",
  "version": "1.0",
  "description": "An extension that makes training listening easier in Duolingo.",
  "content_scripts": [
    {
      "matches": [
        "https://*.duolingo.com/*"
      ],
      "js": ["index.js"]
    }
  ],  
  "icons": {
    "128": "duo.png"
  }
}
